# Staging Environment Variables
# Copy this file to .env.staging.local and fill in your staging values

# Supabase Configuration
VITE_SUPABASE_URL=your_staging_supabase_url
VITE_SUPABASE_ANON_KEY=your_staging_supabase_anon_key

# Application Configuration
VITE_APP_NAME=Ticket Management Portal (Staging)
VITE_APP_VERSION=1.0.0-staging
VITE_APP_ENVIRONMENT=staging

# API Configuration
VITE_API_BASE_URL=https://your-staging-api.com
VITE_API_TIMEOUT=30000

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,video/mp4,video/avi,video/mov,application/pdf,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document,text/plain,audio/mpeg,audio/wav,audio/ogg

# Security Configuration
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Feature Flags
VITE_ENABLE_VOICE_NOTES=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# Performance Configuration
VITE_ENABLE_SERVICE_WORKER=false
VITE_ENABLE_OFFLINE_MODE=false
VITE_CACHE_DURATION=1800000

# Monitoring and Analytics (Staging)
VITE_SENTRY_DSN=your_staging_sentry_dsn_here
VITE_GOOGLE_ANALYTICS_ID=
VITE_HOTJAR_ID=